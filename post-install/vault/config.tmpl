ui = true

listener "tcp" {
    tls_disable     = "true"
    address         = "[::]:8200"
    cluster_address = "[::]:8201"
}

storage "dynamodb" {
    ha_enabled = "true"
    region  = "${region}"
    table   = "${dynamodb_table}"
}

service_registration "kubernetes" {}

seal "awskms" {
    region      = "${region}"
    kms_key_id  = "${kms_key_id}"
}